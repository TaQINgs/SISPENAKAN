{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    login() {\n      const credentials = {\n        email: this.email,\n        password: this.password\n      };\n      axios.post('http://127.0.0.1:8000/api/login', credentials).then(response => {\n        console.log(response);\n        const user = response.data.user;\n        localStorage.setItem('user', JSON.stringify(user));\n        console.log(localStorage);\n\n        // Mengarahkan pengguna ke halaman \"dashboard\" setelah login berhasil\n        if (user.level === 'admin') {\n          this.$router.push('/DashboardAdmin');\n        } else {\n          this.$router.push('/LandingPage2');\n        }\n      }).catch(error => {\n        console.log(error.response.data.message);\n        // Tambahkan logika atau tangani kesalahan jika login gagal\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","email","password","methods","login","credentials","post","then","response","console","log","user","localStorage","setItem","JSON","stringify","level","$router","push","catch","error","message"],"sources":["D:\\Project BPF\\my-project\\my-project\\src\\login.vue"],"sourcesContent":["<!-- eslint-disable vue/multi-word-component-names -->\r\n<template>\r\n  <div class=\"full-page-background\">\r\n    <span class=\"mask bg-gradient-dark opacity-6\"></span>\r\n    <div class=\"container my-auto\">\r\n      <div class=\"row\">\r\n        <div class=\"col-lg-4 col-md-8 col-12 mx-auto\">\r\n          <div class=\"card z-index-0 fadeIn3 fadeInBottom\">\r\n            <div class=\"card-header p-0 position-relative mt-n4 mx-3 z-index-2\">\r\n              <div style=\"background: linear-gradient(-20deg, #00cdac 0%, #8ddad5 100%);\"\r\n                class=\"border-radius-lg py-3 pe-1\">\r\n                <h4 class=\"text-white font-weight-bolder text-center mt-2 mb-0\">Selamat Datang</h4>\r\n              </div>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <form @submit.prevent=\"login\" role=\"form\" class=\"text-start\">\r\n                <div class=\"input-group input-group-outline my-3\">\r\n                  <label class=\"form-label\">Email</label>\r\n                  <input type=\"email\" class=\"form-control\" v-model=\"email\" required>\r\n                </div>\r\n                <div class=\"input-group input-group-outline mb-3\">\r\n                  <label class=\"form-label\">Password</label>\r\n                  <input type=\"password\" class=\"form-control\" v-model=\"password\" required>\r\n                </div>\r\n                <div class=\"text-center\">\r\n                  <button type=\"submit\"\r\n                    style=\"background: radial-gradient(circle at 10% 20%, rgb(10, 174, 227) 0%, rgb(124, 182, 255) 100.2%);\"\r\n                    class=\"btn bg-gradient-primary w-100 my-4 mb-2\">Masuk</button>\r\n                </div>\r\n                <div class=\"text-center\">\r\n                  <router-link to=\"/register\" class=\"btn btn-link text-black\">Belum punya akun? Daftar di\r\n                    sini</router-link>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      const credentials = {\r\n        email: this.email,\r\n        password: this.password,\r\n      };\r\n\r\n      axios.post('http://127.0.0.1:8000/api/login', credentials)\r\n        .then(response => {\r\n          console.log(response);\r\n          const user = response.data.user;\r\n          localStorage.setItem('user', JSON.stringify(user));\r\n          console.log(localStorage);\r\n\r\n          // Mengarahkan pengguna ke halaman \"dashboard\" setelah login berhasil\r\n          if (user.level === 'admin') {\r\n            this.$router.push('/DashboardAdmin');\r\n          } else {\r\n            this.$router.push('/LandingPage2');\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.log(error.response.data.message);\r\n          // Tambahkan logika atau tangani kesalahan jika login gagal\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.full-page-background {\r\n  background-image: url('https://cdn.pixabay.com/photo/2020/07/23/01/29/books-5430104_1280.jpg');\r\n  background-size: cover;\r\n  height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  position: sticky;\r\n}\r\n\r\n.container {\r\n  color: #fff;\r\n  text-align: center;\r\n  padding: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AA2CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,KAAKA,CAAA,EAAG;MACN,MAAMC,WAAU,GAAI;QAClBJ,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACjB,CAAC;MAEDH,KAAK,CAACO,IAAI,CAAC,iCAAiC,EAAED,WAAW,EACtDE,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;QACrB,MAAMG,IAAG,GAAIH,QAAQ,CAACR,IAAI,CAACW,IAAI;QAC/BC,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACJ,IAAI,CAAC,CAAC;QAClDF,OAAO,CAACC,GAAG,CAACE,YAAY,CAAC;;QAEzB;QACA,IAAID,IAAI,CAACK,KAAI,KAAM,OAAO,EAAE;UAC1B,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,iBAAiB,CAAC;QACtC,OAAO;UACL,IAAI,CAACD,OAAO,CAACC,IAAI,CAAC,eAAe,CAAC;QACpC;MACF,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdX,OAAO,CAACC,GAAG,CAACU,KAAK,CAACZ,QAAQ,CAACR,IAAI,CAACqB,OAAO,CAAC;QACxC;MACF,CAAC,CAAC;IACN;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}