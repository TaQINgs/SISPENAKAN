\n\r\n  <!-- Edit Modal -->\r\n  <div class=\"modal fade\" id=\"editModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"editModalLabel\" aria-hidden=\"true\">\r\n    <div class=\"modal-dialog\" role=\"document\">\r\n      <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n          <h5 class=\"modal-title\" id=\"editModalLabel\">Edit Data Donasi</h5>\r\n          <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\" @click=\"closeEditModal\">\r\n            <span aria-hidden=\"true\">&times;</span>\r\n          </button>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n          <!-- Form to edit data -->\r\n          <div class=\"form-group\">\r\n            <label for=\"nama\">Nama</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"nama\" v-model=\"formData.name\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"email\">Email</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"email\" v-model=\"formData.email\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"password\">Password</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"password\" v-model=\"formData.password\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"password\">Alamat</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"password\" v-model=\"formData.alamat\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"instansi\">Instansi</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"instansi\" v-model=\"formData.instansi\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"jenis_pendidikan\">Jenis Pendidikan</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"jenis_pendidikan\" v-model=\"formData.jenis_pendidikan\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"pendapatan\">Pendapatan</label>\r\n            <input type=\"number\" class=\"form-control\" id=\"pendapatan\" v-model=\"formData.pendapatan\">\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"level\">Level</label>\r\n            <input type=\"text\" class=\"form-control\" id=\"level\" v-model=\"formData.level\">\r\n          </div>\r\n          <!-- Add other form fields here based on your data structure -->\r\n\r\n        </div>\r\n        <div class=\"modal-footer\">\r\n          <button type=\"button\" class=\"btn btn-secondary\" data-dismiss=\"modal\" @click=\"closeEditModal\">Close</button>\r\n          <button type=\"button\" class=\"btn btn-primary\" @click=\"saveEditedItem\">Save changes</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      donasiData: [],\r\n      formData: {\r\n        name: '',\r\n        email: '',\r\n        password: '',\r\n        alamat: '',\r\n        instansi: '',\r\n        jenis_pendidikan: '',\r\n        pendapatan: '',\r\n        level: ''\r\n      },\r\n      isEditModalVisible: false,\r\n      editItemId: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.lihatDataDonasi();\r\n  },\r\n  methods: {\r\n    lihatDataDonasi() {\r\n      axios.get('http://127.0.0.1:8000/api/getDataUser')\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.donasiData = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    mounted() {\r\n    this.lihatDataDonasi();\r\n    },\r\n    openEditModal(id) {\r\n      this.editItemId = id;\r\n      this.editItem(id); // Call the editItem method to populate the form fields with the data of the selected item.\r\n    },\r\n    editItem(id) {\r\n      const data = this.donasiData.find(item => item.id === id);\r\n      if (data) {\r\n        this.formData = {\r\n          id: data.id,\r\n          name: data.name,\r\n          email: data.email,\r\n          password: data.password,\r\n          alamat: data.alamat,\r\n          instansi: data.instansi,\r\n          jenis_pendidikan: data.jenis_pendidikan,\r\n          pendapatan: data.pendapatan,\r\n          level: data.level,\r\n        };\r\n\r\n        this.isEditModalVisible = true;\r\n      }\r\n    },\r\n    saveEditedItem() {\r\n      const id = this.formData.id;\r\n      axios\r\n        .put(`http://127.0.0.1:8000/api/User/${id}`, this.formData)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.isEditModalVisible = false;\r\n          this.resetForm();\r\n          this.lihatDataDonasi();\r\n          window.location.reload();\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    closeEditModal() {\r\n      \r\n      this.resetForm();\r\n    }\r\n  }\r\n};\r\n</script>\r\n"],"mappings":"AAqGA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,gBAAgB,EAAE,EAAE;QACpBC,UAAU,EAAE,EAAE;QACdC,KAAK,EAAE;MACT,CAAC;MACDC,kBAAkB,EAAE,KAAK;MACzBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,eAAe,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACPD,eAAeA,CAAA,EAAG;MAChBf,KAAK,CAACiB,GAAG,CAAC,uCAAuC,EAC9CC,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAClB,IAAI,CAAC;QAC1B,IAAI,CAACC,UAAS,GAAIiB,QAAQ,CAAClB,IAAI;MACjC,CAAC,EACAqB,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDT,OAAOA,CAAA,EAAG;MACV,IAAI,CAACC,eAAe,CAAC,CAAC;IACtB,CAAC;IACDS,aAAaA,CAACC,EAAE,EAAE;MAChB,IAAI,CAACZ,UAAS,GAAIY,EAAE;MACpB,IAAI,CAACC,QAAQ,CAACD,EAAE,CAAC,EAAE;IACrB,CA