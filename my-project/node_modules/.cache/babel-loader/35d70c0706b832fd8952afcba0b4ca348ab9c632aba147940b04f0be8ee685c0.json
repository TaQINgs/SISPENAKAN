         <h4 class=\"text-white font-weight-bolder text-center mt-2 mb-0\">Form Registrasi</h4>\r\n              </div>\r\n            </div>\r\n            <div class=\"card-body\">\r\n              <form @submit.prevent=\"register\">\r\n                <div class=\"col-21\">\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"email\">Alamat Email</label>\r\n                    <input type=\"email\" id=\"email\" class=\"form-control\" v-model=\"email\" placeholder=\"Email\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"name\">Nama</label>\r\n                    <input type=\"text\" id=\"name\" class=\"form-control\" v-model=\"name\" placeholder=\"Nama\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"alamat\">Alamat</label>\r\n                    <input type=\"text\" id=\"alamat\" class=\"form-control\" v-model=\"alamat\" placeholder=\"Alamat\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"instansi\">Instansi</label>\r\n                    <input type=\"text\" id=\"instansi\" class=\"form-control\" v-model=\"instansi\" placeholder=\"Instansi\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"jenis_pendidikan\">Jenis Pendidikan</label>\r\n                    <input type=\"text\" id=\"jenis_pendidikan\" class=\"form-control\" v-model=\"jenis_pendidikan\" placeholder=\"Jenis Pendidikan\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"pendapatan\">Pendapatan</label>\r\n                    <input type=\"text\" id=\"pendapatan\" class=\"form-control\" v-model=\"pendapatan\" placeholder=\"Pendapatan\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"password\">Password</label>\r\n                    <input type=\"password\" id=\"password\" class=\"form-control\" v-model=\"password\" placeholder=\"Password\" required>\r\n                  </div>\r\n\r\n                  <div class=\"mb-3\">\r\n                    <label for=\"no_hp\">No HP</label>\r\n                    <input type=\"text\" id=\"no_hp\" class=\"form-control\" v-model=\"no_hp\" placeholder=\"No HP\" required>\r\n                  </div>\r\n\r\n                  <div class=\"text-center\">\r\n                    <button type=\"submit\" class=\"btn bg-gradient-primary w-100 my-4 mb-2\">Registrasi</button>\r\n                    <p>Sudah punya akun?</p>\r\n                    <router-link to=\"/login\" class=\"nav-link btn bg-success text-white\">\r\n                      Login\r\n                    </router-link>\r\n                  </div>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: '',\r\n      password: '',\r\n      confirmPassword: '',\r\n      name: '',\r\n      alamat: '',\r\n      instansi: '',\r\n      jenis_pendidikan: '',\r\n      pendapatan: '',\r\n      no_hp: '',\r\n    };\r\n  },\r\n  methods: {\r\n    register() {\r\n\r\n      const userData = {\r\n        name: this.name,\r\n        email: this.email,\r\n        password: this.password,\r\n        alamat: this.alamat,\r\n        instansi: this.instansi,\r\n        jenis_pendidikan: this.jenis_pendidikan,\r\n        pendapatan: this.pendapatan,\r\n        no_hp: this.no_hp,\r\n      };\r\n\r\n      axios.post('http://127.0.0.1:8000/api/register', userData)\r\n        .then(response => {\r\n          const user = response.data.user;\r\n          localStorage.setItem('user', JSON.stringify(user));\r\n          // Tambahkan logika atau arahkan ke halaman lain setelah registrasi berhasil\r\n          this.$router.push('/login');\r\n        })\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.full-page-background {\r\n  background-image: url('https://cdn.pixabay.com/photo/2020/07/23/01/29/books-5430104_1280.jpg');\r\n  background-size: cover;\r\n  height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n}\r\n\r\n.container {\r\n  color: #fff;\r\n  text-align: center;\r\n  padding: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAyEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,EAAE;MACnBC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,gBAAgB,EAAE,EAAE;MACpBC,UAAU,EAAE,EAAE;MACdC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAAA,EAAG;MAET,MAAMC,QAAO,GAAI;QACfR,IAAI,EAAE,IAAI,CAACA,IAAI;QACfH,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBG,MAAM,EAAE,IAAI,CAACA,MAAM;QACnBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,gBAAgB,EAAE,IAAI,CAACA,gBAAgB;QACvCC,UAAU,EAAE,IAAI,CAACA,UAAU;QAC3BC,KAAK,EAAE,IAAI,CAACA;MACd,CAAC;MAEDV,KAAK,CAACc,IAAI,CAAC,oCAAoC,EAAED,QAAQ,EACtDE,IAAI,CAACC,QAAO,IAAK;QAChB,MAAMC,IAAG,GAAID,QAAQ,CAACf,IAAI,CAACgB,IAAI;QAC/BC,YAAY,CAACC,OAAO,CAAC,MAAM,EAAEC,IAAI,CAACC,SAAS,CAACJ,IAAI,CAAC,CAAC;QAClD;QACA,IAAI,CAACK,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;MAC7B,CAAC;IACL;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}